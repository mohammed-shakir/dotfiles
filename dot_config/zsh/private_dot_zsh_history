: 1753117878:0;sudo tee /usr/lib/systemd/system-sleep/nvidia-reload.sh <<'EOF'\
#!/usr/bin/env bash\
case "$1/$2" in\
  pre/*)\
    # unload before freezing\
    modprobe -r nvidia_drm nvidia_modeset nvidia_uvm nvidia\
    ;;\
  post/*)\
    # reload immediately after resume\
    modprobe nvidia\
    modprobe nvidia_modeset\
    modprobe nvidia_uvm\
    modprobe nvidia_drm\
    ;;\
esac\
EOF
