: 1753174711:0;sudo tee /usr/lib/systemd/system-sleep/nvidia-unload.sh > /dev/null << 'EOF'\
#!/bin/bash\
case "$1/$2" in\
  pre/*)\
    # Stop any services holding the GPU (e.g. your greeter)\
    systemctl stop greetd.service\
    # Unload NVIDIA modules\
    /usr/bin/modprobe -r nvidia_drm nvidia_uvm nvidia_modeset nvidia\
    ;;\
  post/*)\
    # Reload NVIDIA modules\
    /usr/bin/modprobe nvidia\
    /usr/bin/modprobe nvidia_uvm\
    /usr/bin/modprobe nvidia_modeset\
    /usr/bin/modprobe nvidia_drm\
    # Restart your greeter to bring back the login screen\
    systemctl start greetd.service\
    ;;\
esac\
EOF
