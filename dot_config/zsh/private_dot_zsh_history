: 1753118187:0;sudo tee /usr/lib/systemd/system-sleep/hibernate-fix.sh <<'EOF'\
#!/usr/bin/env bash\
# $1 = pre/post, $2 = suspend|hibernate|hybrid-sleep\
if [[ "$1" == post && "$2" == hibernate ]]; then\
  # reload nvidia driver\
  modprobe nvidia\
  modprobe nvidia_modeset\
  modprobe nvidia_uvm\
  modprobe nvidia_drm\
  # respawn Hyprland via HyDE\
  su - shakir -c '\
    export XDG_RUNTIME_DIR=/run/user/$(id -u shakir)\
    hyde-shell hyprland &\
  '\
fi\
EOF
